<p id="notice"><%= notice %></p>

<h1>Set Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Priority</th>
      <th>Complete</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @set_tasks.each do |set_task| %>
      <tr>
        <td><%= set_task.name %></td>
        <td><%= set_task.priority %></td>
        <td><%= set_task.complete %></td>
        <td><%= set_task.user %></td>
        <td><%= link_to 'Show', set_task %></td>
        <td><%= link_to 'Edit', edit_set_task_path(set_task) %></td>
        <td><%= link_to 'Destroy', set_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Set Task', new_set_task_path %>
